/* This is a .Prm file for the QT4*/

NAMES END 
           
SEGMENTS
  MY_RAM     = READ_WRITE  0x0080 TO 0x00FF; 
  MY_ROM     = READ_ONLY   0xEE00 TO 0xFDFF;
END

PLACEMENT
  DEFAULT_ROM     INTO MY_ROM;
  SSTACK          INTO MY_RAM; 
  DEFAULT_RAM     INTO MY_RAM;
END

STACKSIZE 0x8

VECTOR ADDRESS 0xFFFE _Reset
VECTOR ADDRESS 0xFFF2 _TIMER_IRQ


INIT _Reset


